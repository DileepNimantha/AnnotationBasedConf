<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	<bean id="student" class="com.dileep.Student">
		<property name="name" value="Dileep Rajapaksha" />
		<property name="age" value="26"></property>
	</bean>

	<bean id="textEditor" class="com.dileep.TextEditor">
	</bean>

	<bean id="SpellChecker" class="com.dileep.SpellChecker">
	</bean>

	<bean id="profile" class="com.dileep.Profile">
	</bean>

	<bean id="student1" class="com.dileep.Student">
		<property name="name" value="Dileep"></property>
		<property name="age" value="26"></property>
	</bean>

	<bean id="student2" class="com.dileep.Student">
		<property name="name" value="Shane"></property>
		<property name="age" value="25"></property>
	</bean>

</beans>